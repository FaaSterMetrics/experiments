        "calls": [
          "getcart",
          "getproduct",
          "currency",
          "shipmentquote",
          "payment",
          "shiporder",
          "email",
          "emptycart"
        ]
      },
      "email": {
        "provider": "azure"
      },
      "currency": {
        "provider": "azure"
      },
      "supportedcurrencies": {
        "provider": "azure"
      },
      "shiporder": {
        "provider": "azure"
      },
      "shipmentquote": {
        "provider": "azure"
      },
      "payment": {
        "provider": "azure"
      },
      "listproducts": {
        "provider": "azure"
      },
      "getads": {
        "provider": "azure"
      },
      "getproduct": {
        "provider": "azure"
      },
      "searchproducts": {
        "provider": "azure"
      },
      "listrecommendations": {
        "provider": "azure"
      },
